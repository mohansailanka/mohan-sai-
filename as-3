n=int(input("enter number of elements:"))
l=list(map(int,input("enter elements :").split()))
x=int(input("Enter value :"))
low=0
high=n-1
flag=0
mid=(low+high)//2
while high>=low:
    if x>l[mid]:
        low=mid+1
    elif x==l[mid]:
        flag=1
        break
    else:
        high=mid-1
    mid=(low+high)//2
if flag==1:
    print('Element is found at',mid,'Index location')
else:
    print('Element is not found')
